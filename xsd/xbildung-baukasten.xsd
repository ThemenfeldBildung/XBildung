              namespace="urn:xoev-de:kosit:xoev:datentyp:din-91379_2022-08"/>
   <xs:complexType name="Benotung">
      <xs:annotation>
         <xs:documentation>Die Klasse Note dient zur Angabe von Bewertungen aller Art, z.B. Schulnoten auf dem Schulzeugnis, Punktzahlen für ein Modul an der Hochschule, Durchschnittsnoten, Abschlussnoten, Zensuren für Abschlussarbeiten, Prüfungsleistungen und Prüfungsteile in der beruflichen Bildung, etc.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="note" minOccurs="0" type="xs:string" form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld enhält die Bewertung für eine bestimmte Leistung.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="wortbezeichnung"
                     minOccurs="0"
                     type="xbd:String.Localized"
                     form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld enhält die textuelle Bezeichnung für eine bestimmte Note, z.B. "sehr gut" für den Wert 1</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="punkte" minOccurs="0" type="xs:decimal" form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld enhält die Punkte für eine bestimmte Leistung.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="bewertungsschemaNote" minOccurs="0" form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld enhält Angaben zum Schema, nach dem eine Note gegeben wird oder wurde</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="schemaURI" minOccurs="0" type="xs:anyURI">
                     <xs:annotation>
                        <xs:documentation>Dieses Feld enhält einen URI-Verweis auf das zugrundeliegende Bewertungsschema (Grading Scheme). XBildung stellt in der Codeliste Bewertungsschema (Seite ) eine Reihe von schemaURIs zur Verfügung.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="beschreibung" minOccurs="0" type="xbd:String.Localized">
                     <xs:annotation>
                        <xs:documentation>Dieses Feld dient der textuellen Beschreibung des zugrundeliegenden Bewertungsschemas und sollte insbesondere dann genutzt werden, wenn das Bewertungsschema nicht durch eine URI hinterlegt werden kann.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="weitereInformationen"
                              minOccurs="0"
                              maxOccurs="unbounded"
                              type="xs:anyURI">
                     <xs:annotation>
                        <xs:documentation>Hier sollen Links zu Webseiten angegeben werden, die das Bewertungsschema genauer beschreiben.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="besteBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, welche Bewertung (Note, Punktzahl, ...) in diesem Bewertungsschema (Grading Scheme) die theoretisch bestmögliche Bewertung ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bestandenAbBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, ab welcher Bewertung (Note, Punktzahl, ....) in diesem Bewertungsschema (Grading Scheme) eine Leistung als bestanden gilt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="schlechtesteBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, welche Bewertung (Note, Punktzahl, ....) in diesem Bewertungsschema (Grading Scheme) die theoretisch schlechteste Bewertung ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="bewertungsschemaPunkte" minOccurs="0" form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld enhält Angaben zum Schema, nach dem Punkte vergegeben werden oder wurden</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="schemaURI" minOccurs="0" type="xs:anyURI">
                     <xs:annotation>
                        <xs:documentation>Dieses Feld enhält einen URI-Verweis auf das zugrundeliegende Bewertungsschema (Grading Scheme). XBildung stellt in der Codeliste Bewertungsschema (Seite ) eine Reihe von schemaURIs zur Verfügung.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="beschreibung" minOccurs="0" type="xbd:String.Localized">
                     <xs:annotation>
                        <xs:documentation>Dieses Feld dient der textuellen Beschreibung des zugrundeliegenden Bewertungsschemas und sollte insbesondere dann genutzt werden, wenn das Bewertungsschema nicht durch eine URI hinterlegt werden kann.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="weitereInformationen"
                              minOccurs="0"
                              maxOccurs="unbounded"
                              type="xs:anyURI">
                     <xs:annotation>
                        <xs:documentation>Hier sollen Links zu Webseiten angegeben werden, die das Bewertungsschema genauer beschreiben.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="besteBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, welche Bewertung (Note, Punktzahl, ...) in diesem Bewertungsschema (Grading Scheme) die theoretisch bestmögliche Bewertung ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bestandenAbBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, ab welcher Bewertung (Note, Punktzahl, ....) in diesem Bewertungsschema (Grading Scheme) eine Leistung als bestanden gilt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="schlechtesteBewertung" minOccurs="0" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hier wird angegeben, welche Bewertung (Note, Punktzahl, ....) in diesem Bewertungsschema (Grading Scheme) die theoretisch schlechteste Bewertung ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="beschreibung"
                     minOccurs="0"
                     type="xbd:String.Localized"
                     form="qualified">
            <xs:annotation>
               <xs:documentation>Dieses Feld soll genutzt werden, um unstrukturierte Informationen über die Note zu übermitteln.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Code.AmtlicherGemeindeschluessel">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Gemeinden, dargestellt durch den Amtlichen Gemeindeschlüssel (AGS) des Statistischen Bundesamtes</nameLang>
               <nameKurz>AGS</nameKurz>
               <nameTechnisch>AGS</nameTechnisch>
