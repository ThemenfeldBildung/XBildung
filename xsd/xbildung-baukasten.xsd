   <xs:complexType name="Dokument">
      <xs:annotation>
         <xs:documentation>Allgemeine Eigenschaften von Dokumenten, die zum Zwecke generischerer Verarbeitung übergreifend modelliert wurden</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="identifikation"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xbd:Identifikation">
            <xs:annotation>
               <xs:documentation>eindeutige Identifikation eines Dokuments im Bildungswesen</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="sprache"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xbd:Sprache">
            <xs:annotation>
               <xs:documentation>Sprachkennzeichnung des Dokuments. Bitte nutzen Sie Eintragungen nach "BCP47" (https://tools.ietf.org/html/bcp47) zur Kennzeichnung der Sprache(n) in der die Bescheinigung ausgestellt ist. Damit sind Eintragungen wie DE-AT; CA-FR möglich.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ausstellung" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Angaben zur Ausstellung des Dokuments</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="datum" minOccurs="0" type="xs:date">
                     <xs:annotation>
                        <xs:documentation>Datum der Ausstellung des Dokuments</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="ort" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Ort der Ausstellung des Dokuments</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="gueltigkeitszeitraum" minOccurs="0" type="xbd:Zeitraum">
            <xs:annotation>
               <xs:documentation>Gibt an, in welechem Zeitraum das Dokument gültig ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="titel"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xbd:String.Localized">
            <xs:annotation>
